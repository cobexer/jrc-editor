
JAVAC=javac
JCFLAGS=-d ../classes -classpath $(CLASSPATH);../classes
Jack=javacc

OUTROOT=../classes
SRCROOT=../src

all: tools-compile compile                                              
                                                                                
install:
    cp $(SRCROOT)/*.properties $(OUTROOT)
    cd $(OUTROOT)
    zip -r9 jrc-editor.jar *.properties org/*
clean:
    rm -rf $(OUTROOT)/*.properties $(OUTROOT)/org $(OUTROOT)/*.jar

dist-clean:
    rm -rf $(OUTROOT)/*.properties $(OUTROOT)/org

tools-compile: \
    $(SRCROOT)/org/zaval/tools/i18n/translator/JavaParser.flags \
    $(SRCROOT)/org/zaval/tools/i18n/translator/UtfParser.flags 

JCCLIST1=\
    $(SRCROOT)/org/zaval/tools/i18n/translator/JavaParser.java              \
